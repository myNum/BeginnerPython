from random import randint

highest = 10
answer = randint(0,highest)
guess = input("Guess a number between 0 - %d: " %highest)

while (int(guess) != answer):
	if (int(guess) < answer):
		print("Your guess is too low!")
	else:
		print("Your guess it to high!")

	guess = input("Guess a number between 0 - %d: " %highest)

print("YOU WIN! WOOP WOOP!")
